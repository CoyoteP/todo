create sequence "TODOID_SEQ"
    start with 1
    increment by 1
    maxvalue 10000
    minvalue 1
    nocache
    cycle
    noorder
    
create table todo(
	todoid VARCHAR(255) primary key,
	body VARCHAR(255),
	userid VARCHAR(255)
)

create table users(
	userid VARCHAR(255) primary key,
	password VARCHAR(255),
	birthday VARCHAR(255),
	username VARCHAR(255),
	enable varchar(255),
    role varchar(255)
)

create table todo(
    todoid varchar(255) primary key,
    body varchar(255),
    todoflg varchar(255),
    userid varchar(255)
    )
